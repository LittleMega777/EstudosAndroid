<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.appsegurana.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_security"
        android:name="com.example.appsegurana.ui.dashboard.SecurityFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_security" />

    <fragment
        android:id="@+id/navigation_config"
        android:name="com.example.appsegurana.ui.notifications.ConfigFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_config" />
</navigation>